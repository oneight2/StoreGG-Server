<table id="example2" class="table table-bordered table-hover">
  <thead>
    <tr>
      <th>No</th>
      <th>Nama Game</th>
      <th>Category</th>
      <th>Nominal</th>
      <th>Status</th>
      <th>Aksi</th>
    </tr>
  </thead>
  <tbody>
    <% var i = 0 %> <% voucher.forEach(field=>{ %>
    <tr>
      <td><%= i+1 %></td>
      <td><%= field.name %></td>
      <td><%= field?.category?.name ?? '-' %></td>
      <td>
        <% field.nominals.forEach(field =>{ %>
        <span class="badge badge-sm badge-info"><%= field.coinName %></span>
        <% }) %>
      </td>
      <td>
        <% if(field.status ==='Y'){ %>
        <span class="badge badge-sm badge-success">Aktif</span>
        <% }else{%>
        <span class="badge badge-sm badge-danger">Non Aktif</span>
        <% } %>
      </td>
      <td class="text-center">
        <form
          action="/voucher/delete/<%= field._id %>?_method=DELETE"
          method="post"
        >
          <a
            class="btn btn-primary btn-sm"
            href="/voucher/update/<%= field._id %>"
          >
            Update
          </a>
          <button class="btn btn-danger btn-sm">hapus</button>
        </form>
      </td>
    </tr>
    <% }) %>
  </tbody>
  <tfoot>
    <tr>
      <th>No</th>
      <th>Nama Game</th>
      <th>Kategori</th>
      <th>Nominal</th>
      <th>Status</th>
      <th>Aksi</th>
    </tr>
  </tfoot>
</table>
